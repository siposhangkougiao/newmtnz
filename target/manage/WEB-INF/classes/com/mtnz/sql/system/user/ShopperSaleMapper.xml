<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ShopperSaleMapper">

	<insert id="saveLevel">
		insert into shopper_sale (uid,type,level,product_id) values (#{uid},#{type},#{level},#{product_id})
	</insert>

</mapper>